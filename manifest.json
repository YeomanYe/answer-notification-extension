{
   "background": {
      "scripts": ["background.js"]
   },
   "browser_action": {
       "default_icon": "images/icon/logo32.png",
      "default_popup": "popup.html",
      "default_title": "PostTrack"
   },
   "content_scripts": [{
      "js": ["lib/jquery.min.js", "cnt.js"],
      "matches": ["http://*/*", "https://*/*"],
      "run_at": "document_end"
   }],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "icons": {
      "16": "images/icon/logo16.png",
      "32": "images/icon/logo32.png",
      "48": "images/icon/logo48.png",
      "64": "images/icon/logo64.png",
      "128": "images/icon/logo128.png"
   },
   "manifest_version": 2,
   "name": "PostTrack",
   "options_page": "popup.html",
   "permissions": ["http://*/*", "https://*/*", "notifications", "storage", "unlimitedStorage","tabs"],
   "version": "1.0.0",
   "web_accessible_resources": ["remote/*", "js/*","css/*","lib/*", "manifest.json", "images/*"]
}
